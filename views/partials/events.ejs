<section id="promo">
    <div id="label">
        Upcoming Shows
        <hr>
    </div>
    <section id="events">
        <% if(events == null || events.length == 0) { %>
            <h2 style="text-align:center; font-size: 120%">Nothing coming up right now.</h2>
        <% }
        else 
        {%>
        <% for(let i = 0; i < events.length; i++){ %>
        <section class="event">
            <img class="promoIMG" src="<%- events[i].event_promo %>">
            <div style="text-align:center">
                <h2><%- events[i].event_name %></h2>
                <p><%- events[i].event_date.toLocaleDateString("en-US") %><br> <%- events[i].event_location %> </p>
            </div>
        </section>
        <% } %>
        <% } %>
    </section>
</section>